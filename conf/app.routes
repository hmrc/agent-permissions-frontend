# microservice specific routes

->         /hmrc-frontend                   hmrcfrontend.Routes

GET        /assets/*file                    controllers.Assets.versioned(path = "/public", file: Asset)

GET        /                                controllers.RootController.start

GET        /opt-in/start                    controllers.OptInController.start
GET        /opt-in/do-you-want-to-opt-in    controllers.OptInController.showDoYouWantToOptIn
POST       /opt-in/do-you-want-to-opt-in    controllers.OptInController.submitDoYouWantToOptIn

GET        /opt-in/you-have-opted-in        controllers.OptInController.showYouHaveOptedIn
GET        /opt-in/you-have-not-opted-in       controllers.OptInController.showYouHaveNotOptedIn
